
<%= form_for @user, url: signin_path do |f| %>
<%= f.label :name %>
<%= f.text_field :name, id: "name" %>

<%= f.label :password %>
<%= f.password_field :password, id: "password" %>
<%= f.submit 'Sign In', :html => {:class => 'btn btn-outline-primary'} %><%# if     @contact.errors.any? %>
<% end %>
